{
  "name": "code-git",
  "displayName": "code-git",
  "description": "Git integration",
  "version": "0.0.1",
  "publisher": "ilkka",
  "engines": {
    "vscode": "^1.7.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openGitStatus"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openGitStatus",
        "title": "Code-Git Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "simple-git": "^1.62.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
